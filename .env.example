# Environment Variable Template

# Set to true to write to the remote Cloudflare D1 database during scraping
REMOTE_DB=false

# Cloudflare Configuration
CLOUDFLARE_ACCOUNT_ID=
CLOUDFLARE_API_TOKEN=
CLOUDFLARE_DATABASE_ID=

# NextAuth Configuration
# Run `npx auth secret` or `openssl rand -base64 32` to generate one
AUTH_SECRET=

# OAuth Providers
GITHUB_CLIENT_ID=
GITHUB_CLIENT_SECRET=
GOOGLE_CLIENT_ID=
GOOGLE_CLIENT_SECRET=
