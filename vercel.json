{
  "framework": "nextjs",
  "buildCommand": "npm run build",
  "devCommand": "npm run dev",
  "installCommand": "npm install",
  "git": {
    "ignoreCommand": "if [ \"$VERCEL_GIT_COMMIT_REF\" == \"dev\" ]; then exit 0; else exit 1; fi"
  },
  "crons": [
    {
      "path": "/api/cron/daily-reminder",
      "schedule": "0 6 * * *"
    }
  ]
}
